package com.kh.variable;

public class D_Printf {
	
	public void printfTest() {
		/*
		 * println() => 값 출력 후 자동 개행(\n)
		 * print() => 값 만 출력한 후 종료 ( 개행문X )
		 * printf("출력할 형식", 형식에 맞는 값들 ...)
		 *	=> f는 format을 의미.
		 *	=> 형식에 맞춰서 값들을 출력한 후 종료 (줄바꿈 x)
		 *	문자열에 추가 가능한 형식
		 *	%d : 정수
		 *	%f : 실수
		 *	%c : 문자
		 * 	%s : 문자열
		 * */
		// 정수 테스트
		int iNum1 = 10;
		int iNum2 = 20;
		
		//println문 사용
		System.out.println("iNum : "+iNum1+", iNum2 : "+iNum2);
		//printf문 사용
		System.out.printf("iNum1 : %d , iNum2 : %d" ,iNum1,iNum2);
		
		
		
	}

}
