package com.kh.practice1.func;

public class CastingPractice3 { // 기능 제공용 클래스
		public void method() {
		int iNum1 = 10;
		int iNum2 = 4;

		float fNum = 3.0f;

		double dNum = 2.5;

		char ch = 'A';
		
		
		int a1 = iNum1 / iNum2;
		System.out.println( a1 ); // 2
		
		int a2 = (int)dNum;
		System.out.println( a2 ); // 2

		
		
		double b1 = iNum2*2 + dNum;
		System.out.println( b1 ); // 10.0
		
		double b2 = iNum1;
		System.out.println( b2 ); // 10.0

		
		double c = iNum1 / iNum2;
		System.out.println( c ); // 2.5
		System.out.println( dNum ); // 2.5

		
		System.out.println( fNum ); // 3
		
		int d = (int)(iNum1 / fNum);
		System.out.println( d ); // 3
		
		
		float e1 = iNum1 / fNum;
		System.out.println( e1 );// 3.3333333
		
		double e2 = iNum1 / fNum;
		System.out.println( e2 ); // 3.3333333333333335

		System.out.println( ch ); // 'A'
		int f1 = (int)ch;
		System.out.println( f1 ); // 65
		int f2 = (int)ch+iNum1;
		System.out.println( f2 ); // 75
		char f3 = (char)f2;
		System.out.println( f3 ); // 'K'

	 }
}
