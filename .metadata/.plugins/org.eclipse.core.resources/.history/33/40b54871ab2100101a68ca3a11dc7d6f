package com.kh.chap04_Constructor.model.vo;

public class User {
	// 필드부
	private String userId;
	private int age;
	private char gender;
	
	// 생성자부
	/*
	 * 생성자 작성방법
	 * 
	 * 접근제한자 클래스명 (매개변수){
	 * 	// 생성자를 통해 객체 생성시 초기화 시킬 필드값들을 나열
	 * }
	 * 
	 * 작성목적 
	 * 1. 객체를 생성하기 위한 목적
	 * 2. 객체 생성과 객체 내부의 필드값을 초기화할 목적
	 * */
	//기본생성자
	public User(/*기본생성자*/){
		// 객체를 생성할 목적으로만 사용
		// 이 기본생성자는 작성하지 않아도 오류가 나지 않음.
		// JVM이 자동으로 만들어 줬기 때문에 문제가 없던것.
		// 단, 내가 생성자를 한개 이상 만들어둔 경우 JVM이 
		// 자동생성해주지 않다.
		
		// 메서드부
	}
	public User(Stirng userId, int age, char gender) {
		this.userId = userId;
		this.age = age;
		this.gender = gender;
	}
	

}
