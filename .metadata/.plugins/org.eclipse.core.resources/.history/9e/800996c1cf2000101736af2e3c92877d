package com.kh.array;

public class C_DimensionalArray {
	
	// 이차원 배열 :일차원 배열을 묶음으로 관리하는 배열
	
	public void method1() {
		//이차원 배열 선언
		int[][]arr1;	//이걸로 주로사용
		int arr2 [][];
		int [] arr3 [];
		
		// 이차원 배열 할당
		// 배열 = new 자료형[행크기][열크기]
		// 배열 = new 자료형 [2차원배열크기][1차원배열크기]
		
		arr3 = new int[2][3];
		// 3개의 정수값을 관리하는 2차원 배열 2개를 관릴하는 2차원 배열
		
		// 배열 선언과 동시에 할당
		int[][] arr = new int [3][5];
		
		System.out.println(arr);
		
		
	}

}
